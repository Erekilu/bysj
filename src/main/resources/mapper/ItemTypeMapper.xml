<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.hbue.bysj.mapper.ItemTypeMapper">
	<select id="selectItemType" resultType="ItemType">
		select *
		from item_type
		where item_type_category = #{category}
	</select>
</mapper>